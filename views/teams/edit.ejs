<div>
  <h2>Edit Team <%= team.name %></h2>

  <form action="/teams/<%= team.name %>" class="put-form">
    <div class="form-group">
      <label for="name">Name:</label>
      <input type="text" name="name" id="name" class="form-control" value="<%= team.name %>">
    </div>
    <div class="form-group">
      <label for="members">Members:</label>
      <input type="text" name="members" id="members" class="form-control" value="<%= team.members %>">
    </div>
    <input type="submit" class="btn btn-primary" value="Update Team Info">
  </form>
</div>